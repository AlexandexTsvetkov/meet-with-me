@namespace("ru.aston.meet.kafka.notifications.invitation")
protocol InvitationProtocol {

	record InvitationAvro {
		string id;
		timestamp_ms timestamp;
		string invitedEmail;
		string invitedName;
		string title;
		string description;
		timestamp_ms eventDate;
		string initiatorName;
		string location;
	}
}